<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sreetej.mypractise">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.WRITE_CONTACTS"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"

        android:theme="@style/AppTheme">
        <activity android:name=".activity.RecyclerViewActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".dataBindingSample.DataBindingExampleActivity" />
        <activity android:name=".compoundViewSample.CompoundViewActivity" />
        <activity android:name=".runtimePermissionSample.RuntimePermissionSampleActivity" />
        <activity android:name=".dialogsInFragmentExample.DiagInFragActicity" />
        <activity android:name=".retrofitSample.RectrofitSampleActivity" />
        <activity android:name=".retrofitSampleStackOverFlowEg.RetrofitSOflowSampleActivity" />

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity android:name=".loaderSample.LoadContactsUsingLoaderActivity"></activity>
    </application>

</manifest>